{
  "manifest_version": 2,
  "name": "Dom Size Analyzer",
  "description": "Google Chrome DevTools Extension for analyzing DOM size",
  "version": "1.1",
  "permissions": ["*://*/*"],
  "devtools_page": "devtools.html",
  "web_accessible_resources": ["injectable.js"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "background": {
    "scripts": ["backgroundScript.js"],
    "persistent": false
  }
}
